{
	"name": "zod-commander",
	"version": "0.0.3",
	"description": "A TypeScript utility for building type-safe CLI commands using commander and zod.",
	"author": "Rom√°n Via-Dufresne Saus <roman910dev@gmail.com>(https://github.com/roman910dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/roman910dev/zod-commander.git"
	},
	"keywords": [
		"commander",
		"zod",
		"cli",
		"typescript"
	],
	"devDependencies": {
		"@biomejs/biome": "^2.0.6",
		"@jest/globals": "^30.0.4",
		"@types/node": "^22.16.3",
		"commander": "^14.0.0",
		"husky": "^9.1.7",
		"jest": "^30.0.4",
		"ts-jest": "^29.4.0",
		"typescript": "^5.8.3",
		"zod": "^3.25.0"
	},
	"packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist/"
	],
	"imports": {
		"#/*": "./src/*"
	},
	"scripts": {
		"build": "tsc --project tsconfig.build.json",
		"lint": "biome check .",
		"lint:fix": "biome check --fix --formatter-enabled=false .",
		"format": "biome format --write .",
		"ci": "biome ci --threads=4",
		"prepublishOnly": "npm run build",
		"prepare": "husky",
		"test": "jest"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"commander": ">=8 <15",
		"zod": ">=3.20.0 <4"
	}
}
